    "typescript": "^5.5.4",
    "unplugin-auto-import": "^0.18.2",
    "unplugin-vue-components": "^0.27.3",
    "unplugin-vue-router": "^0.10.2",
    "vite": "^5.3.5",
    "vite-plugin-vue-meta-layouts": "^0.4.3",
    "vite-plugin-vuetify": "^2.0.4",
    "vitest": "^2.0.5",
    "vue-tsc": "^2.0.29"
