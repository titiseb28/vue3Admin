    "vue-echarts": "7.0.1",
    "vue-router": "^4.4.2",
    "vuetify": "^3.6.14"
  },
  "devDependencies": {
    "@mdi/js": "^7.4.47",
    "@nuxt/eslint-config": "0.5.0",
    "@pinia/testing": "^0.1.4",
    "@playwright/test": "^1.45.3",
    "@testing-library/vue": "^8.1.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.14",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.8.0",
